generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model istla_tipo_beca {
  id                     Int                      @id @default(autoincrement())
  tipo_beca              String                   @db.VarChar(255)
  istla_solicitudes_beca istla_solicitudes_beca[]
}

model istla_solicitudes_beca {
  id                        Int                         @id @default(autoincrement())
  fecha_hora                DateTime                    @db.DateTime(0)
  tipo_beca                 Int
  cedula                    String                      @db.VarChar(20)
  istla_documento_solicitud istla_documento_solicitud[]
  istla_tipo_beca           istla_tipo_beca             @relation(fields: [tipo_beca], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "istla_solicitudes_beca_ibfk_1")

  @@index([tipo_beca], map: "tipo_beca")
}

model istla_documento_solicitud {
  id                     Int                    @id @default(autoincrement())
  id_solicitud_beca      Int
  documento              Bytes?
  istla_solicitudes_beca istla_solicitudes_beca @relation(fields: [id_solicitud_beca], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "istla_documento_solicitud_ibfk_1")

  @@index([id_solicitud_beca], map: "id_solicitud_beca")
}
